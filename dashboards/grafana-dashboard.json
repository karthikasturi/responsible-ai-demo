        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "tooltip": false,
                "viz": false,
                "legend": false
              },
              "scaleDistribution": {
                "type": "linear"
              }
            }
          }
        }
      },
